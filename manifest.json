{
    "manifest_version": 2,
    "name": "Rater Cf",
    "version": "1.0",
    "description": "Change the color of a div to red",
    "permissions": [
        "activeTab",
        "http://localhost:3000/*", 
        "https://localhost:3000/*" 
      ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  